{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "reputly.app"
        }
      ],
      "destination": "https://www.reputly.app/:1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "http://www.reputly.app"
        }
      ],
      "destination": "https://www.reputly.app/:1",
      "permanent": true
    }
  ]
}
